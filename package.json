{
  "name": "rovoweb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "mocha node_modules/mocha/bin/_mocha --require @babel/register $(find ./src -name '*-test.js')",
    "start": "node dist/server/app",
    "start-dev": "nodemon --watch ./src/server --exec \"NODE_ENV=development node src/server/dev-entry | npx bunyan\"",
    "build-node": "NODE_ENV=production npx boring-build-node",
    "build-client": "NODE_ENV=production npx boring-build-client",
    "build": "npm run build-node && npm run build-client",
    "build-scratch": "npm run build-node && npm run up && npm run build-db && npm run build-client",
    "build-db": "node src/scripts/create-db.js",
    "up": "docker-compose up -d",
    "debug": "NODE_ENV=development node --inspect-brk src/server/dev-entry",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanstevens/rovoweb.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ryanstevens/rovoweb/issues"
  },
  "homepage": "https://github.com/ryanstevens/rovoweb#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "config": "^2.0.1",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "hot-module-replacement": "^3.0.1",
    "mocha": "^5.2.0",
    "proxyquire": "^2.1.0",
    "webpack-hot-middleware": "^2.23.1"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^3.1.0",
    "auth0": "^2.12.0",
    "aws-sdk": "^2.322.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "boringbits": "git://github.com/ryanstevens/boring.git",
    "bunyan": "^1.8.12",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "dynamoose": "^0.8.7",
    "express-session": "^1.15.6",
